{
    "name": "mamazu/doctrine-performance",
    "type": "phpstan-extension",
    "description": "Rules for doctrine query performance.",
    "licence": "MIT",
    "require": {
        "doctrine/persistence": "^4.0",
        "phpstan/phpstan": "^2.0",
        "symfony/cache": "^7.2"
    },
    "require-dev": {
        "doctrine/orm": "^3.3",
        "phpunit/phpunit": "^12.1",
        "symplify/easy-coding-standard": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "Mamazu\\DoctrinePerformance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Mamazu\\DoctrinePerformance\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "mamazu",
            "email": "14860264+mamazu@users.noreply.github.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "phpstan": {
            "includes": [
                "rules.neon"
            ]
        }
    }
}
